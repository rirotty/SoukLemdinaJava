<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.souklemdina.gui.WishListFXMLController">
    <stylesheets>
        <URL value="@wishlistfxml.css" />
    </stylesheets>
   <children>
      <TableView fx:id="tableWishList" layoutY="-1.0" prefHeight="318.0" prefWidth="600.0">
        <columns>
          <TableColumn fx:id="produitImage" prefWidth="207.0" text="C1" />
          <TableColumn fx:id="produitDesc" prefWidth="366.0" text="C2" />
        </columns>
      </TableView>
      <Button fx:id="btnAjout" layoutX="14.0" layoutY="343.0" mnemonicParsing="false" onMouseClicked="#ajouterAuPanier" text="Ajouter au Panier" />
      <Button fx:id="btnSupp" layoutX="135.0" layoutY="343.0" mnemonicParsing="false" onMouseClicked="#supprimerLignesDePanier" text="Supprimer" />
      <Button fx:id="bntPanier" layoutX="509.0" layoutY="343.0" mnemonicParsing="false" onAction="#goToPanier" text="Mon Panier" />
   </children>
</AnchorPane>
